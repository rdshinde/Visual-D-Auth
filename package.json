{
  "name": "react-visual-d-auth",
  "version": "1.0.0",
  "description": "NPM package for your projects decentralised graphical password authentication.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rdshinde/Visual-D-Auth.git"
  },
  "keywords": [
    "reactjs",
    "npm",
    "authentication",
    "grahical-password",
    "d-auth"
  ],
  "author": "Rishikesh Shinde",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rdshinde/Visual-D-Auth/issues"
  },
  "homepage": "https://github.com/rdshinde/Visual-D-Auth#readme",
  "peerDependencies": {
    "react": ">=16"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.28",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-dom": "^18.0.11",
    "@types/web3": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "autoprefixer": "^10.4.14",
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "crypto-js": "^4.1.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "framer-motion": "^10.6.1",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.6",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-dotenv": "^0.1.3",
    "react-hot-toast": "^2.4.0",
    "react-uuid": "^2.0.0",
    "tailwindcss": "^3.2.7",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2",
    "web3": "^1.9.0"
  }
}
