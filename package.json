{
  "name": "react-visual-d-auth",
  "version": "1.0.19",
  "description": "NPM package for your projects decentralised graphical password authentication.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs && postcss src/styles/tailwind.css -o dist/styles.css",
    "build:styles": "NODE_ENV=production npx --ignore-existing postcss src/styles/tailwind.css -o dist/styles.css",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:assets": "cp -R src/assets dist/esm/assets",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "test": "jest --config jestconfig.json",
    "prepare": "rm -r ./dist && npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/react-beautiful-dnd": "^13.1.3",
    "@types/react-dom": "^18.0.10",
    "@types/web3": "^1.2.2",
    "bcryptjs": "^2.4.3",
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "framer-motion": "^9.1.5",
    "mnemonic-words": "^1.1.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dotenv": "^0.1.3",
    "react-hot-toast": "^2.4.0",
    "react-uuid": "^2.0.0",
    "rollup": "^3.20.2",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4",
    "web3": "^1.9.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rdshinde/Visual-D-Auth.git"
  },
  "keywords": [
    "reactjs",
    "npm",
    "authentication",
    "grahical-password",
    "d-auth"
  ],
  "author": "Rishikesh Shinde",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rdshinde/Visual-D-Auth/issues"
  },
  "homepage": "https://github.com/rdshinde/Visual-D-Auth#readme",
  "peerDependencies": {
    "react": ">=17",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@babel/runtime": "7.15.4",
    "@testing-library/react": "^14.0.0",
    "@types/autoprefixer": "^10.2.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "autoprefixer": "^9.8.8",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.5.0",
    "npm": "^9.6.2",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.4.3"
  }
}
